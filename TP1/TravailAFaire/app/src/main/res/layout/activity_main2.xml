<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity2">
    <TextView
        android:id="@+id/email_resultat"
        android:layout_width="320dp"
        android:layout_height="56dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="15dp"
        android:text="TextView"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nom_resultat" />
    <TextView
        android:id="@+id/adress_resultat"
        android:layout_width="320dp"
        android:layout_height="56dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="15dp"
        android:text="TextView"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/phone_resultat" />
    <TextView
        android:id="@+id/ville_resultat"
        android:layout_width="320dp"
        android:layout_height="56dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="15dp"
        android:text="TextView"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/adress_resultat" />
    <TextView
        android:id="@+id/nom_resultat"
        android:layout_width="320dp"
        android:layout_height="56dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="30dp"
        android:text="TextView"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
    <TextView
        android:id="@+id/phone_resultat"
        android:layout_width="320dp"
        android:layout_height="56dp"
        android:layout_marginLeft="50dp"
        android:layout_marginTop="15dp"
        android:text="TextView"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/email_resultat" />
</androidx.constraintlayout.widget.ConstraintLayout>